<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="scene" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles/base.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BookManagement.EditBook">
   <children>
      <TextField fx:id="tfID" disable="true" layoutX="207.0" layoutY="124.0" prefHeight="25.0" prefWidth="196.0">
         <font>
            <Font name="Calibri" size="14.0" />
         </font></TextField>
      <TextField fx:id="tfTitle" layoutX="207.0" layoutY="194.0" prefHeight="25.0" prefWidth="196.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <TextField fx:id="tfPrice" layoutX="207.0" layoutY="266.0" prefHeight="25.0" prefWidth="196.0">
         <font>
            <Font size="14.0" />
         </font></TextField>
      <Label id="header" alignment="CENTER" layoutX="209.0" layoutY="20.0" text="Edit Book">
         <font>
            <Font name="Calibri Bold" size="46.0" />
         </font>
      </Label>
      <Button id="cancel" layoutX="351.0" layoutY="347.0" mnemonicParsing="false" onAction="#moveToBookManagement" text="Cancel">
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
         </padding>
      </Button>
      <Button id="save" layoutX="178.0" layoutY="347.0" mnemonicParsing="false" onAction="#editBook" text="Save">
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
         </padding>
      </Button>
      <Label layoutX="210.0" layoutY="98.0" text="ID">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <Label layoutX="211.0" layoutY="172.0" text="Title">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
      <Label layoutX="210.0" layoutY="245.0" text="Price">
         <font>
            <Font name="Calibri" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
